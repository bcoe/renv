{
  "name": "renv",
  "version": "2.0.0",
  "description": "a command line interface for managing remote configuration, powered by etcd",
  "main": "renv.js",
  "scripts": {
    "test": "nyc --require babel-core/register mocha --check-leaks --timeout 5000",
    "prepublish": "babel index.js > renv.js"
  },
  "bin": "./bin/renv.js",
  "config": {
    "blanket": {
      "pattern": "renv.js",
      "data-cover-never": [
        "node_modules",
        "test"
      ],
      "output-reporter": "spec"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bcoe/renv.git"
  },
  "keywords": [
    "etcd",
    "configuration",
    "config",
    "ndm"
  ],
  "author": "Ben Coe <ben@npmjs.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bcoe/renv/issues"
  },
  "homepage": "https://github.com/bcoe/renv",
  "dependencies": {
    "bluebird": "^3.0.5",
    "chalk": "^1.1.1",
    "columnify": "^1.4.1",
    "deep-extend": "^0.4.0",
    "inquirer": "^0.11.0",
    "lodash": "^3.3.0",
    "node-etcd": "^4.2.1",
    "quack-array": "0.0.1",
    "rc": "^1.1.5",
    "traverse": "^0.6.6",
    "window-size": "^0.1.0",
    "yargs": "^3.5.3"
  },
  "devDependencies": {
    "babel-cli": "^6.2.4",
    "babel-core": "^6.2.1",
    "babel-preset-es2015": "^6.1.18",
    "chai": "^3.4.1",
    "coveralls": "^2.11.2",
    "mocha": "^2.3.4",
    "nyc": "^4.0.1"
  }
}
